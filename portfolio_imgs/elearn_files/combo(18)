),placeholderDescription:s.get("placeholderDescription"),buttonTextCreate:s.get("buttonTextCreate"),buttonTextCancel:s.get("buttonTextCancel"),photoUrl:t.getSizeToFit({width:80,height:80},{includeSquare:!0,includeSizes:["sq","q"]}).url})),o=n.one(".album-creation-title"),r!==""&&(o.set("value",r),s.handleTitleChange({newVal:r})),o.focus(),e.rapidTracker.beacon(s.name,s.get("type")+"AlbumSelectionSwitchModeClick")}))},handleCancelClick:function(t){var n=this.get("container");n.one(".album-creation").hide(),n.one(".album-selection").show(),this.get("container").one(".album-selection-search").focus(),e.rapidTracker.beacon(this.name,this.get("type")+"AlbumSelectionCancelClick")},handleAdditionClick:function(t){t.preventDefault();var n=t.currentTarget.getAttribute("data-album-id"),r=t.currentTarget,i=this;r.addClass("will-belong-to-album"),this.switchDoneButton(),this.addPhoto(n).then(function(e){r.addClass("belongs-to-album"),r.removeClass("will-belong-to-album")}).then(null,function(e){r.removeClass("will-belong-to-album"),i.appContext.error(e.message,e.stack)}),e.rapidTracker.beacon(this.name,this.get("type")+"AlbumSelectionAdditionClick")},handleRemovalClick:function(t){t.preventDefault();var n=t.currentTarget.getAttribute("data-album-id"),r=t.currentTarget,i=this;r.removeClass("belongs-to-album"),this.switchDoneButton(),this.removePhoto(n).then(function(e){e||(r.remove(),i.handleSearch())}).then(null,function(e){r.addClass("belongs-to-album")}),e.rapidTracker.beacon(this.name,this.get("type")+"AlbumSelectionRemovalClick")},handleNewSetClick:function(t){var n=this,r=this.get("container"),i=r.one(".album-creation-title").get("value"),s=r.one(".album-creation-description").get("value"),o,u,a,f;r.one(".album-creation-create").setAttribute("disabled"),this.switchDoneButton(),this.createNewAndAddPhoto(i,s).then(function(e){r.one(".album-creation").remove(),r.one(".album-selection").show(),u=n.getTitleLengthClass(e.getValue("title"))+" ",f=e.registry.attributes.compoundId?